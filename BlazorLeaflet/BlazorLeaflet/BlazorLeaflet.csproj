<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Version>0.5.0-alpha</Version>
    <Authors>Mihai Stan</Authors>
    <Company />
    <Description>BlazorLeaflet is a wrapper offering easy-to-use Blazor components that expose the Leaflet API in C#. Target .NET 6.0</Description>
    <Copyright>Mihai Stan</Copyright>
    <PackageProjectUrl></PackageProjectUrl>    
    <RepositoryUrl>https://github.com/djrodriguezperez/BlazorLeaflet-net6.0</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageId>BlazorLeaflet-net6</PackageId>
    <PackageTags>blazor, leaflet, c#, js, interop</PackageTags>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageLicenseExpression></PackageLicenseExpression>
    <LangVersion>8</LangVersion>
    <IsPackable>true</IsPackable>
  </PropertyGroup>


  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components" Version="6.0.5" />
    <PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="6.0.5" />
  </ItemGroup>

</Project>
